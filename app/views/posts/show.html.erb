<div class="posts">
  <div class="container">

    <div class="show-box">
      <div class="post-header">

        <div class="post-information">
          <div class="image">
            <%= image_tag(@post.image.url, alt: 'Image') if @post.image? %>
          </div>
          <div class="post-info-box">
            <div class="post-t-p-r">
              <%= react_component 'A_locationDescriptionBlack', {text: @post.title, color: '#000 '} %>
              <%= react_component 'A_priceTag', {price: @post.price, color: '#4F6DDB '} %>
              <div class="image">8.8</div>
            </div>
            <div class="post-c-t-l">
              <%= react_component 'A_locationDescription', {text: @post.city, color: '#4F6DDB '} %>
              <div class="time-by-pass">
                <img src="/assets/Time.png" alt="">
                <span>Время тура: <%= @post.timeBypass %></span>
              </div>
              <div class="shows">
                <div class="views">
                  <img src="/assets/eye.png" alt="">
                  <span>100 просмотров</span>
                </div>
                <div class="visits">
                  <img src="/assets/portfel.png" alt="">
                  <span>10 посещений</span>
                </div>
              </div>
            </div>
            <div class="post-author">
              <img src="/assets/ProfileAvatarBlue.svg" alt="">
              <div class="author-username">
                Автор: <%=  User.find(@post.user_id).username %>
              </div>
              <%= link_to 'Профиль', User.find(@post.user_id) %>
            </div>
            <div class="likes-favorites">
              <div class="like">
                Оценили: <%= @post.likes.count %>
              </div>
              <div class="likes">
                <% if user_signed_in? %>
                  <% pre_like = @post.likes.find { |like| like.user_id == current_user.id} %>
                  <% if pre_like %>
                    <%= button_to 'Снять отметку', post_like_path(@post, pre_like), method: :delete %>
                  <% else %>
                    <%= button_to 'Нравится', post_likes_path(@post), method: :post %>
                  <% end %>
                <% end %>
              </div>
              <div class="favorites">
                <% if user_signed_in? %>
                  <% pre_favorite = @post.favorites.find { |favorite| favorite.user_id == current_user.id} %>
                  <% if pre_favorite %>
                    <%= button_to 'Удалить из избранного', post_favorite_path(@post, pre_favorite), method: :delete %>
                  <% else %>
                    <%= button_to 'Добавить в избранное', post_favorites_path(@post), method: :post %>
                  <% end %>
                <% end %>
              </div>
              <div class="edited">
                <% if current_user.id == @post.user_id || current_user.isadmin == true%>
                  <%= link_to 'Редактировать', edit_post_path(@post) %>
                  <%= link_to 'Удалить', @post, method: :delete, data: { confirm: 'Are you sure?' } %>
                <% end %>
              </div>
            </div>
          </div>
        </div>


<!--      <div class="comment-box">-->
<!--        <div class="comments">-->
<!--          <h2>Comments</h2>-->
          <%#= render @post.comments %>
<!--        </div>-->

